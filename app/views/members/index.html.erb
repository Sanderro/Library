<nav>
  <ul>
    <li><%= link_to "Strona glowna", "/welcome/index" %></li>
    <li><%= link_to "Lista ksiazek", "/books/index"%></li>
    <li><%= active_link_to "Czlonkowie", "/members/index"%></li>
    <li><%= link_to "Wypozyczenia", "/borrows/index"%></li>
  </ul>
</nav>
<main><h2>Lista czlonkow</h2>
  <table class="table-list">
    <thead>
    <tr>
      <th>Imie</th>
      <th>Nazwisko</th>
      <th>Email</th>
    </tr>
    </thead>
    <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.firstname %></td>
        <td><%= member.lastname %></td>
        <td><%= member.email %></td>
        <td><ul class="list-actions">
          <li>
            <%= link_to "Szczegoly", "/members/#{member.id}"%>
          </li>
          <li>lol
            <%= link_to "Edytuj", edit_member_path(member.id) %>

          </li>
          <li>
            <%= link_to "Usun", "/members/#{member.id}", method: :delete, data: { confirm: "Usunac?"} %>
          </li>
        </ul></td>
      </tr>
    <%end%>
    </tbody>
  </table>
  <div><p><%= link_to "Dodaj uzytkownika", "/members/new", class: "button-add" %></p></div>
  <p></p></main>
